<driverGroup xmlns="http://www.sun.com/japex/testSuite">
    <param name="japex.classPath" value="${japex.exi.ttfms.frameworkLibDir}/*.jar"/>
    <param name="japex.classPath" value="${japex.exi.ttfms.candidatesDir}/xml/dist/candidate-xml-jaxp.jar"/>
    
    <param name="japex.driverClass" value="org.w3c.exi.ttf.candidate.xml.jaxp.JAXPSAXDriver"/>
    <param name="org.w3c.exi.ttf.driver.candidateName" value="XML-JAXP"/>
    <param name="org.w3c.exi.ttf.driver.isXmlProcessor" value="true"/>

    <driver name="XMLJAXPDocumentSAX" normal="true">
        <param name="description" value="XML JAXP using document analysis"/>
        <param name="org.w3c.exi.ttf.applicationClass" value="document"/>
        <param name="org.w3c.exi.ttf.applicationClass.documentAnalysing.GZIP" value="true"/>
    </driver>
</driverGroup>

