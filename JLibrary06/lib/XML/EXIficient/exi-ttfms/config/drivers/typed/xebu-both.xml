<driverGroup xmlns="http://www.sun.com/japex/testSuite">
    <param name="japex.classPath" value="${japex.exi.ttfms.frameworkLibDir}/*.jar"/>
    <param name="japex.classPath" value="${japex.exi.ttfms.candidatesDir}/xebu/dist/candidate-xebu.jar"/>
    <param name="japex.classPath" value="${japex.exi.ttfms.candidatesDir}/xebu/lib/*.jar"/>
    
    <param name="japex.driverClass" value="org.w3c.exi.ttf.candidate.xebu.XebuCustomDriver"/>
    <param name="org.w3c.exi.ttf.driver.candidateName" value="Xebu"/>

    <driver name="XebuBothCustom">
	    <param name="description" value="Schema-based Xebu format with gzip through custom API"/>
        <param name="org.w3c.exi.ttf.applicationClass" value="both"/>
        <param name="org.w3c.exi.ttf.driver.documentAnalysing.GZIP" value="true"/>
    </driver>

</driverGroup>

