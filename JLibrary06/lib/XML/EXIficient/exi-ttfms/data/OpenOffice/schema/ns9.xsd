      <xs:attribute ref="ns11:to"/>
      <xs:attribute ref="ns11:from"/>
      <xs:attribute ref="ns11:by"/>
      <xs:attribute ref="ns12:type" use="required"/>
      <xs:attribute ref="ns11:fill"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="transitionFilter">
    <xs:complexType>
      <xs:attribute ref="ns11:targetElement"/>
      <xs:attribute name="sub-item" form="qualified" type="xs:string"/>
