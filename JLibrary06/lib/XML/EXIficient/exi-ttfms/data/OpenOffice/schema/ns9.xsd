      <xs:attribute ref="ns10:master-element"/>
      <xs:attribute ref="ns10:group-id"/>
      <xs:attribute name="id" form="qualified" type="xs:ID"/>
      <xs:attribute ref="ns11:begin"/>
      <xs:attribute ref="ns11:end"/>
