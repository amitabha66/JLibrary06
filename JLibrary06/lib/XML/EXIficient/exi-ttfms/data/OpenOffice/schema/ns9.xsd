      <xs:attribute ref="ns11:direction"/>
      <xs:attribute ref="ns11:fadeColor"/>
      <xs:attribute ref="ns11:mode"/>
      <xs:attribute ref="ns11:fill"/>
    </xs:complexType>
  </xs:element>
