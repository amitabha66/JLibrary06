      </xs:choice>
      <xs:attribute ref="ns10:node-type"/>
      <xs:attribute ref="ns10:preset-id"/>
      <xs:attribute ref="ns10:preset-sub-type"/>
      <xs:attribute ref="ns10:preset-class"/>
      <xs:attribute ref="ns10:master-element"/>
      <xs:attribute ref="ns10:group-id"/>
      <xs:attribute name="id" form="qualified" type="xs:ID"/>
