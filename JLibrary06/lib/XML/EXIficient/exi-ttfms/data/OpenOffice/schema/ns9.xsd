      <xs:attribute ref="ns11:repeatDur"/>
      <xs:attribute ref="ns11:repeatCount"/>
      <xs:attribute ref="ns11:restart"/>
      <xs:attribute ref="ns11:restartDefault"/>
      <xs:attribute ref="ns11:fill"/>
      <xs:attribute ref="ns11:fillDefault"/>
      <xs:attribute ref="ns11:accelerate"/>
      <xs:attribute ref="ns11:decelerate"/>
      <xs:attribute ref="ns11:autoReverse"/>
      <xs:attribute ref="ns11:endsync"/>
