      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="ns9:animate"/>
        <xs:element ref="ns9:set"/>
        <xs:element ref="ns9:animateMotion"/>
        <xs:element ref="ns9:animateColor"/>
        <xs:element ref="ns9:animateTransform"/>
        <xs:element ref="ns9:transitionFilter"/>
        <xs:element ref="ns9:par"/>
        <xs:element ref="ns9:seq"/>
        <xs:element ref="ns9:iterate"/>
