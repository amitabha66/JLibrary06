      <xs:attribute ref="ns11:additive"/>
      <xs:attribute ref="ns11:values"/>
      <xs:attribute name="formula" form="qualified" type="xs:string"/>
      <xs:attribute ref="ns11:to"/>
      <xs:attribute ref="ns11:from"/>
      <xs:attribute ref="ns11:by"/>
      <xs:attribute ref="ns11:calcMode"/>
      <xs:attribute ref="ns11:keyTimes"/>
      <xs:attribute ref="ns11:keySplines"/>
