        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="rgb"/>
            <xs:enumeration value="hsl"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="color-interpolation-direction" form="qualified">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="clockwise"/>
            <xs:enumeration value="counter-clockwise"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute ref="ns11:fill"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="animateTransform">
    <xs:complexType>
      <xs:attribute ref="ns11:targetElement"/>
      <xs:attribute name="sub-item" form="qualified" type="xs:string"/>
      <xs:attribute ref="ns11:attributeName" use="required"/>
      <xs:attribute ref="ns11:accumulate"/>
      <xs:attribute ref="ns11:additive"/>
