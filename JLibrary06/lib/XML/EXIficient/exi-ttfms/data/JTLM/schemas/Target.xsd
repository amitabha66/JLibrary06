			<xs:enumeration value="RECONNAISSANCE"/>
			<xs:enumeration value="ELECTRONIC_WARFARE"/>
			<xs:enumeration value="ASSAULT_SUPPORT"/>
			<xs:enumeration value="AIRDROP"/>
			<xs:enumeration value="AIR_INTERDICTION"/>
