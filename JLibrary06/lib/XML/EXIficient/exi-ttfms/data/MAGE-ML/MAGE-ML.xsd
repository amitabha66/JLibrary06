      <xs:group ref="Identifiable_content"/>
      <xs:element minOccurs="0" ref="Roles_assnlist"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="Contact_attrs">
    <xs:attributeGroup ref="Identifiable_attrs"/>
    <xs:attribute name="URI"/>
    <xs:attribute name="address"/>
    <xs:attribute name="phone"/>
    <xs:attribute name="tollFreePhone"/>
    <xs:attribute name="email"/>
    <xs:attribute name="fax"/>
  </xs:attributeGroup>
