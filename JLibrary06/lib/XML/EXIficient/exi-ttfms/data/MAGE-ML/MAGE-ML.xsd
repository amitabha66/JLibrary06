    <xs:complexType>
      <xs:group ref="QuantitationType_ref"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="QuantitationTypes_assnreflist">
    <xs:complexType>
      <xs:group maxOccurs="unbounded" ref="QuantitationType_ref"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="SourcesQuantitationType_assnreflist">
    <xs:complexType>
      <xs:group maxOccurs="unbounded" ref="QuantitationType_ref"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TargetQuantitationType_assnref">
    <xs:complexType>
      <xs:group ref="QuantitationType_ref"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="QuantitationType_ref">
    <xs:complexType>
      <xs:attribute name="identifier" use="required"/>
      <xs:attribute name="name"/>
    </xs:complexType>
  </xs:element>
  <!--
    Element and Attlist declarations for SpecializedQuantitationType
    
    User defined quantitation type.
    
    Associations
        Inherites associations from base class QuantitationType.
    
    Attributes
        Inherites attributes from base class QuantitationType.
